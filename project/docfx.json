{
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "**/*.png",
          "**/*.html",
          "**/*.zip",
          "**/*.gif",
          "**/*.docx",
          "**/*.txt",
          "**/*.jpg",
          "**/*.jpeg",
          "**/*.csv",
          "**/*.sql"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "Software Engineering",
      "_appTitle": "Software Engineering",
      "_enableSearch": true,
      "pdf": false
    }
  }
}